{
  "name": "ffmpeg",
  "productName": "gffmpeg",
  "version": "0.0.1",
  "description": "Simple video encoder",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . gffmpeg --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=out --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"gffmpeg\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "ffmpeg"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.0"
  },
  "dependencies": {
    "electron-packager": "^13.0.1"
  }
}
